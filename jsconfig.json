{
	"compilerOptions": {
		"target": "es5",
		"lib": ["dom", "es2015"],
		"module": "es2015",
		"moduleResolution": "node",
		"experimentalDecorators": true,
		"strict": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"jsx": "preserve"
	},
	"include": ["./**/*.ts", "./vue/**/*"],
	"compileOnSave": false
}
